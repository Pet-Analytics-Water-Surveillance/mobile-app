{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "env": {
        "EXPO_NO_CAPABILITY_SYNC": "1",
        "NODE_ENV": "production"
      },
      "channel": "production",
      "cache": {
        "disabled": false
      }
    }
  },
  "submit": {
    "production": {}
  }
}
